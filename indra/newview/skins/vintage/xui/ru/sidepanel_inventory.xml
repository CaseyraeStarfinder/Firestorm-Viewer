<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="Вещи" name="objects panel">
	<panel name="sidepanel_inventory_panel">
		<tab_container name="inventory_tab_container">
			<panel label="Инвентарь" name="panel_main_inventory" />
			<panel label="Marketplace" name="inbox_outbox_layout_panel">
				<layout_stack name="inbox_outbox_layout_stack_off">
					<layout_panel name="inbox_layout_panel_off">
						<panel name="marketplace_inbox">
							<string name="InboxLabelWithArg">
								Полученные вещи ([NUM])
							</string>
							<string name="InboxLabelNoArg">
								Полученные вещи
							</string>
							<text value="Полученные вещи" name="inbox_label" />
							<text name="inbox_fresh_new_count">
								Новых: [NUM]
							</text>
							<panel name="inbox_inventory_placeholder_panel" tool_tip="Перетащите вещи в ваш инвентарь для их использования">
								<panel name="inbox_inventory_placeholder">
									Покупки из Marketplace будут доставлены сюда.
								</panel>
							</panel>
						</panel>
					</layout_panel>
					<layout_panel name="outbox_layout_panel">
						<panel name="marketplace_outbox">
							<string name="OutboxLabelWithArg">Исходящие продавца ([NUM])</string>
							<string name="OutboxLabelNoArg">Исходящие продавца</string>
							<text value="Исходящие продавца" name="outbox_label" />
							<button tool_tip="Отправить на Marketplace" name="outbox_sync_btn" />
							<panel name="outbox_inventory_placeholder_upperpanel">
								<panel name="outbox_inventory_placeholder_panel">
									<text name="outbox_inventory_placeholder_title">
										Загрузка...
									</text>
								</panel>
							</panel>
						</panel>
					</layout_panel>
				</layout_stack>
			</panel>
		</tab_container>
	</panel>
</panel>
