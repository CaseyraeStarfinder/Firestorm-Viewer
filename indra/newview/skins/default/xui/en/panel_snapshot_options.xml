<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="all"
 height="240"
 layout="topleft"
 name="panel_snapshot_options"
 width="208">
 <layout_stack
  animate="false"
  name="option_buttons"
  orientation="vertical"
  height="186"
  width="208"
  left="0"
  top="9"
  follows="all"
  layout="topleft">
  <layout_panel
   auto_resize="false"
   user_resize="false"
   top_pad="0"
   height="22"
   name="lp_download">
 <button
   follows="left|top"
   font="SansSerif"
   halign="left"
   height="22"
   image_overlay="Snapshot_Download"
   image_overlay_alignment="left"
   image_top_pad="-0"
   imgoverlay_label_space="10"
   label="Save to Disk"
   layout="topleft"
   left="9"
   name="save_to_computer_btn"
   top_pad="0">
    <button.commit_callback
     function="Snapshot.SaveToComputer" />
