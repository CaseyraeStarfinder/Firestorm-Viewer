<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="perms default" title="デフォルトの作成権限">
	<panel label="デフォルト権限" name="default permissions">
		<view_border /> 
		<text name="txt_next_owner">
			次の所有者：
		</text>
		<text name="txt_copy">
			コピー
		</text>
		<text name="txt_modify">
			修正
		</text>
		<text name="txt_transfer">
			再販・プレゼント
		</text>
		<text name="txt_share_with_group">
			グループ共有
		</text>
		<text name="txt_anyone_copy">
			全員にコピー許可
		</text>
		<text name="txt_objects" tool_tip="オブジェクトを作成する時の権限を初期設定します">
			オブジェクト
		</text>
		<check_box name="objects_c" />
		<check_box name="objects_m" />
		<check_box name="objects_t" />
		<check_box name="objects_s" />
		<check_box name="objects_e" />
		<text name="txt_uploads" tool_tip="アップロードしたアイテムの権限を初期設定します">
			アップロード
		</text>
		<check_box name="uploads_c" />
		<check_box name="uploads_m" />
		<check_box name="uploads_t" />
		<check_box name="uploads_s" />
		<check_box name="uploads_e" />
		<text name="txt_scripts" tool_tip="スクリプトを作成する時の権限を初期設定します">
			スクリプト
		</text>
		<check_box name="scripts_c" />
		<check_box name="scripts_m" />
		<check_box name="scripts_t" />
		<check_box name="scripts_s" />
		<check_box name="scripts_e" />
		<text name="txt_notecards" tool_tip="ノートカードを作成する時の権限を初期設定します">
			ノートカード
		</text>
		<check_box name="notecards_c" />
		<check_box name="notecards_m" />
		<check_box name="notecards_t" />
		<check_box name="notecards_s" />
		<check_box name="notecards_e" />
		<text name="txt_gestures" tool_tip="ジェスチャーを作成する時の権限を初期設定します">
			ジェスチャー
		</text>
		<check_box name="gestures_c" />
		<check_box name="gestures_m" />
		<check_box name="gestures_t" />
		<check_box name="gestures_s" />
		<check_box name="gestures_a" />
		<text name="txt_wearables" tool_tip="衣類や身体部位を作成する時の権限を初期設定します">
			衣類・身体部位
		</text>
		<check_box name="wearables_c" />
		<check_box name="wearables_m" />
		<check_box name="wearables_t" />
		<check_box name="wearables_s" />
		<check_box name="wearables_e" />
  </panel>
	<button label="OK" label_selected="OK" name="ok"/>
	<button label="取り消し" label_selected="取り消し" name="cancel"/>
</floater>
