<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel name="panel_facebook_photo">
	<combo_box name="resolution_combobox" tool_tip="Bild-Auflösung">
		<combo_box.item label="Aktuelles Fenster" name="CurrentWindow" />
		<combo_box.item label="Benutzerdefiniert" name="Custom"/>
	</combo_box>
	<combo_box name="filters_combobox" tool_tip="Bild-Filter">
		<combo_box.item label="Kein Filter" name="NoFilter" />
	</combo_box>
	<check_box label="Seitenverh. behalten" name="keep_aspect_ratio"/>
	<text name="working_lbl">
		Aktualisiere...
	</text>
	<button label="Aktualisieren" name="new_snapshot_btn" tool_tip="Klicken, um zu aktualisieren" />
	<button label="Vorschau" name="big_preview_btn" tool_tip="Klicken, um Vorschau zu öffnen" />
	<text name="caption_label">
		Kommentar (optional):
	</text>
	<button label="Posten" name="post_photo_btn" />
	<button label="Abbrechen" name="cancel_photo_btn" />
</panel>
